{
  "manifest_version": 3,
  "name": "Google Reviews Scraper",
  "version": "1.0",
  "description": "Scrapes Google reviews from the current page......",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://www.google.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
